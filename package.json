  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.4",
    "@types/node": "^16.18.38",
    "@types/qs": "^6.9.7",
    "babel-plugin-module-extension-resolver": "^1.0.0",
    "eslint": "^8.38.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.24.4",
    "typescript": "^4.9.5"
  }
}
