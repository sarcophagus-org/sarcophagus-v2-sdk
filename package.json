{
  "name": "sarcophagus-v2-sdk",
  "version": "0.1.0-alpha.1",
  "main": "dist/node/index.js",
  "browser": "dist/browser/index.js",
  "module": "dist/index.ts",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "license": "UNLICENSED",
  "exports": {
    "./node": {
      "import": {
        "types": "./dist/node/index.d.ts",
        "default": "./dist/node/index.js"
      }
    },
    "./browser": {
      "import": {
        "types": "./dist/browser/index.d.ts",
        "default": "./dist/browser/index.js"
      }
    },
    ".": {
      "browser": "./dist/browser/index.js",
      "import": {
        "types": "./dist/browser/index.d.ts",
        "default": "./dist/browser/index.js"
      }
    },
    "./*": {
      "types": "./*.d.ts",
      "default": "./*.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc",
    "prettier": "npx prettier --write src/**",
    "docs": "typedoc --out docs --readme README.md src"
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "^12.0.0",
    "@libp2p/interface-connection": "^5.0.2",
    "@libp2p/interface-peer-id": "^2.0.2",
    "@libp2p/kad-dht": "^5.0.1",
    "@libp2p/mplex": "^7.0.0",
    "@libp2p/webrtc-star": "^5.0.3",
    "@libp2p/websockets": "^5.0.1",
    "@multiformats/multiaddr": "^10.0.3",
    "@bundlr-network/client": "^0.11.2",
    "@sarcophagus-org/sarcophagus-v2-contracts": "^0.16.0",
    "arbundles": "^0.9.6",
    "arweave": "^1.13.7",
    "ethers": "^5.7.2",
    "isomorphic-fetch": "^3.0.0",
    "it-pipe": "^3.0.1",
    "libp2p": "^0.40.0",
    "moment": "^2.29.4",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^16.11.12",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.4",
    "typescript": "^4.5.4"
  }
}
