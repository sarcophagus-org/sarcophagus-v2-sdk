  "main": "dist/browser.js",
  "browser": "dist/browser.js",
  "type": "module",
  "sideEffects": false,
  "license": "UNLICENSED",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc && npx babel src --out-dir dist --extensions \".ts\" --copy-files",
    "prettier": "npx prettier --write src/** test/**",
    "docs": "typedoc --out docs --readme README.md src"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/test/**/*.test.ts",
      "**/test/**/*.integration.ts"
    ],
    "moduleNameMapper": {
      "libp2p": "<rootDir>/test/__mocks__/libp2pMock.ts"
    }
  },
  "dependencies": {
    "@bundlr-network/client": "^0.11.2",
    "@chainsafe/libp2p-noise": "^12.0.0",
